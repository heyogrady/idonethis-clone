<%= simple_form_for(@notch) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
      <%= f.input :raw_text %>
      <%= f.input :markedup_text %>
      <%= f.input :is_goal %>
      <%= f.input :goal_completed %>
      <%= f.input :completed_at %>
      <%= f.association :team %>
      <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
