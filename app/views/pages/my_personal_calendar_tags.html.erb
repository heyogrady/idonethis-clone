            <p>
              <a class="btn btn-primary" id="recent-tags-show-all">
                Show all Tags
              </a>
            </p>
          </div><!-- /.recent-tags-show-all -->
        </ul><!-- /.list-unstyled -->
      </div><!-- /#hashtag_details -->
    </div><!-- /.col-md-3 -->
    <div class="col-md-8 col-md-push-1">
      <div id="feed">
        <h2>No tag selected</h2>
        <p>Please select a tag on the sidebar.</p>
      </div><!-- /#feed -->
    </div><!-- /.col-md-8 col-md-push-1 -->
